package com.adotaPet.api.dto;


import com.adotaPet.api.domain.Ong;
import com.adotaPet.api.domain.Raca;
import com.adotaPet.api.domain.Animal;
import com.adotaPet.api.domain.Doenca;

public class AnimalDTO {
	
	private Integer id;	
	private Integer codigo;
	private String nome;
	private Integer genero;
	private char porte;
	private char vacinado;
	private char vermifugado;
	private char castrado;	
	private Ong ong;
	private Doenca doenca;
	private Raca raca;
	
	public AnimalDTO() {
	}

	public AnimalDTO(Animal objDto) {
		
		id = objDto.getId();
		this.codigo = codigo;
		this.genero = genero;
		this.porte = porte;
		this.vacinado = vacinado;
		this.vermifugado = vermifugado;
		this.castrado = castrado;
		this.ong = ong;
		this.doenca = doenca;
		this.raca = raca;
		objDto.,objDto.getCodigo(),objDto.getNome(),objDto.getGenero(),objDto.getPorte(),
		objDto.getVacinado(),objDto.getVermifugado(),objDto.getCastrado(),objDto.getOng(),		
		objDto.getDoenca(),objDto.getRaca()
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getCodigo() {
		return codigo;
	}

	public void setCodigo(Integer codigo) {
		this.codigo = codigo;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Integer getGenero() {
		return genero;
	}

	public void setGenero(Integer genero) {
		this.genero = genero;
	}

	public char getPorte() {
		return porte;
	}

	public void setPorte(char porte) {
		this.porte = porte;
	}

	public char getVacinado() {
		return vacinado;
	}

	public void setVacinado(char vacinado) {
		this.vacinado = vacinado;
	}

	public char getVermifugado() {
		return vermifugado;
	}

	public void setVermifugado(char vermifugado) {
		this.vermifugado = vermifugado;
	}

	public char getCastrado() {
		return castrado;
	}

	public void setCastrado(char castrado) {
		this.castrado = castrado;
	}

	public Ong getOng() {
		return ong;
	}

	public void setOng(Ong ong) {
		this.ong = ong;
	}

	public Doenca getDoenca() {
		return doenca;
	}

	public void setDoenca(Doenca doenca) {
		this.doenca = doenca;
	}

	public Raca getRaca() {
		return raca;
	}

	public void setRaca(Raca raca) {
		this.raca = raca;
	}
	
	
	
	
}
